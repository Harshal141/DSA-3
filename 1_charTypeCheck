// Take input as char and output lowercase for a-z, Uppercase for A-Z, numeric for 0-9

#include <iostream>
using namespace std;

int main() {
  char a;
  cin >> a;
  cout << int(a) << endl;
  if(48 <= int(a) && int(a)<= 57 ){
    cout << "Its an Int"  << endl;
  }
  if(65 <= int(a) && int(a)<= 90 ){
    cout << "Its UpperCase Alphabet"  << endl;
  }
  if(97 <= int(a) && int(a)<= 122 ){
    cout << "Its LowerCase Alphabet"  << endl;
  }
  return 0;
}